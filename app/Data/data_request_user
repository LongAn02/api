<- create User ->
{
    name : string
    email : string
    password : string
    password_confirmation : string
    phone : string
    address : string
    age : tinyInteger
    sex : tinyInteger
}


<- update User ->
{
    name : string
    email : string
    password : string
    phone : string
    address : string
    age : tinyInteger
    sex : tinyInteger
    id : userId
}


<- show User ->
{
    id : userId
}

<- delete User ->
{
    id : userId
}
